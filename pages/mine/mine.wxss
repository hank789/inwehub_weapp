.my-info,
.order-info, 
.other-info {
 background-color: #6fb7dc;
}

.my-info,.order-info,.other-info{
    width: 100%;
    background-color:#fff;
    margin-top: 20rpx;
    font-size: 14px;
    line-height: 28px;
    border: solid #e5e5e5;
    border-width: 1px 0;
}

.my-info{
    padding:40rpx;
}
.no-now{
    color: #999;
}

.my-info{
    height:100rpx;
}

.my-name{
    font-size: 16px;
    color:#708090;
    height: 55%;
}

.my-img{
    width:100rpx;
    height:100rpx;
    border-radius: 20rpx;
    float: left;
    margin-right: 20rpx;
}

.my-name,
.my-identification {
    line-height: 50rpx;
}

.info-icon{
     padding: 20rpx 100rpx;   
}

.info-icon:not(:last-child){
    border-bottom: solid 2rpx #e5e5e5;
}

.order-start {
    background: url(data:image/png;base64,) no-repeat 40rpx 32rpx;
}

.order-participate {
    background: url(data:image/png;base64,) no-repeat 40rpx 32rpx;
}

.order-identification {
    background: url(data:image/png;base64,) no-repeat 40rpx 32rpx;
}

.other-invite {
    background: url(data:image/png;base64,) no-repeat 40rpx 32rpx;
}

.other-use {
    background: url(data:image/png;base64,) no-repeat 40rpx 32rpx;
}

.other-about {
    background: url(data:image/png;base64,) no-repeat 40rpx 32rpx;
}