	<view class="detail-bottom">
	    <view class="bottom-box">
	        <view wx:if="{{demand.is_author}}" bindtap="navToHome" class="item">
	            <i class="iconfont icon-home"></i>
	            <view class="doc">首页</view>
	        </view>
	        <view wx:else bindtap="navToMyHistory" class="item">
                <i class="iconfont icon-home"></i>
                <view class="doc">足迹</view>
            </view>

	        <button hover-class="none" plain="true" open-type="share" class="item docbutton">
                <i class="iconfont icon-share"></i>
                <view class="doc">分享</view>
	        </button>
	        <block wx:if="{{demand.is_author}}">
	            <view bindtap="navToRooms" class="item">
                    <i class="iconfont icon-message"></i>
                    <view class="doc">消息</view>
                </view>
	            <view class="sy-bottom btn_cart" bindtap="authCloseDemand">关闭</view>
	            <view class="sy-bottom btn_order" bindtap="authEditDemand">修改</view>
	        </block>
	        <block wx:if="{{!demand.is_author}}">
                <view class="sy-bottom btn_order" bindtap="goContact">和TA聊聊</view>
            </block>
		</view>
	</view>
	<component-share id="component-share-demand" option="{{ shareOption }}"></component-share>
