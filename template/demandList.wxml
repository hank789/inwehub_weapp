<!--需求列表 模版-->
<template name="demandList">
    <view class="weui-panel" wx:if="{{list.length > 0}}">
        <view class="weui-panel__bd">
            <view class="weui-media-box weui-media-box_text" wx:for="{{list}}" wx:key="id" data-id="{{ item.id }} " bindtap="navToDetail">
                <view class="weui-media-box__title weui-media-box__title_in-text">{{ item.title }}</view>
                <view class="weui-media-box__info">
                    <view class="weui-media-box__info__meta"></view>
                    <view class="weui-media-box__info__meta">{{ item.created_at}}</view>
                    <view class="weui-media-box__info__meta weui-media-box__info__meta_extra">{{ item.address }}</view>
                </view>
            </view>
        </view>
    </view>
</template>